{
  "name": "zk-states",
  "version": "0.0.17",
  "license": "MIT",
  "description": "Verifiable state manager for React based on SnarkyJS & Mina protocol",
  "typesVersions": {
    "<4.0": {
      "esm/*": [
        "ts3.4/*"
      ],
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && rollup -c",
    "prettier": "prettier \"*.{js,json,md}\" \"{examples,src,tests,docs}/**/*.{js,jsx,ts,tsx,md,mdx}\" --write",
    "prettier:ci": "prettier '*.{js,json,md}' '{examples,src,tests,docs}/**/*.{js,jsx,ts,tsx,md,mdx}' --list-different",
    "eslint": "eslint --no-eslintrc --c .eslintrc.json --fix '*.{js,json}' '{src,tests}/**/*.{ts,tsx}'",
    "eslint:ci": "eslint  --no-eslintrc --c .eslintrc.json '*.{js,json}' '{src,tests}/**/*.{ts,tsx}'",
    "pretest": "tsc --noEmit",
    "test": "vitest --ui --coverage",
    "test:ci": "vitest",
    "test:debug": "DEBUG=vitest:web-worker vitest"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kryha/zk-states.git"
  },
  "keywords": [
    "react",
    "state",
    "manager",
    "management",
    "store",
    "mina",
    "snark",
    "snarkyjs",
    "o1js",
    "zk",
    "zero-knowledge",
    "proofs"
  ],
  "author": "Marius Sili (https://github.com/silimarius)",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "@vitest/web-worker": "^0.34.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@latest",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest": "^0.2.8",
    "jsdom": "^22.1.0",
    "o1js": "^0.12.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vitest": "^0.34.1",
    "vitest-canvas-mock": "^0.3.3",
    "vitest-fetch-mock": "^0.2.2",
    "zustand": "^4.3.9"
  },
  "peerDependencies": {
    "o1js": ">=0.11.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "zustand": ">=4.3.9"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  }
}
