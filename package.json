{
  "name": "zk-state",
  "packageManager": "yarn@3.6.1",
  "private": true,
  "version": "0.0.1",
  "description": "Global React state management with ZK support",
  "main": "./index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    "<4.0": {
      "esm/*": [
        "ts3.4/*"
      ],
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "files": [
    "**"
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "module": "./esm/index.js",
      "default": "./index.js"
    }
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "concurrently -m 8 'yarn:build:*'",
    "build:base": "rollup -c --bundleConfigAsCjs",
    "prettier": "prettier \"*.{js,json,md}\" \"{examples,src,tests,docs}/**/*.{js,jsx,ts,tsx,md,mdx}\" --write",
    "prettier:ci": "prettier '*.{js,json,md}' '{examples,src,tests,docs}/**/*.{js,jsx,ts,tsx,md,mdx}' --list-different",
    "eslint": "eslint --no-eslintrc --c .eslintrc.json --fix '*.{js,json}' '{src,tests}/**/*.{ts,tsx}'",
    "eslint:ci": "eslint  --no-eslintrc --c .eslintrc.json '*.{js,json}' '{src,tests}/**/*.{ts,tsx}'",
    "pretest": "tsc --noEmit",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kryha/zk-state.git"
  },
  "keywords": [
    "react",
    "state",
    "manager",
    "management",
    "store",
    "mina",
    "snark",
    "snarkyjs",
    "zk",
    "zero-knowledge",
    "proofs"
  ],
  "author": "Marius Sili (https://github.com/silimarius)",
  "dependencies": {
    "snarkyjs": "^0.12.1",
    "zod": "^3.21.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "downlevel-dts": "^0.11.0",
    "esbuild": "^0.18.14",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@latest",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.3",
    "rollup-plugin-esbuild": "^5.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
